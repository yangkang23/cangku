<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.brt.oa.record.dao.RecordDao">
   <insert id="insertRecord" parameterType="com.brt.oa.record.pojo.Record">
       insert into record(product_name,price,amount,project_name,project_cost,total_cost,deal_date,cid)
       values (#{product_name},#{price},#{amount},#{project_name},#{project_cost},#{total_cost},#{deal_date},#{cid});
   </insert>


</mapper>